%!

/TWOUPDICT 32 dict def

TWOUPDICT begin

    % LEFTSIDE false is appropriate for normally two sided documents.

    /LEFTSIDE     true def	% Start on LHS of page?
    /PRESERVE     true def	% Preserve aspect ratio?

    /PAGEWIDTH    8.50 def	% Physical width of the page.
    /PAGEHEIGHT  11.00 def	% Physical height of the page.

    /LPAGEWIDTH   8.50 def	% Width of a page of the input.
    /LPAGEHEIGHT 13.00 def	% Height of a page of the input.

    /XOFFSET      0.00 def	% Fudge factors shouldn't be needed, but...
    /YOFFSET      0.00 def	% Note: offsets are in actual inch units
    /XSADJUST     0.98 def
    /YSADJUST     0.98 def	% Set these slightly < 1 for elbow room.
    /RIGHTFUDGE  -0.50 def	% A negative value shifts RHS to the left.


    /XSCALE PAGEHEIGHT LPAGEWIDTH 2 mul div XSADJUST mul def
    /YSCALE PAGEWIDTH LPAGEHEIGHT       div YSADJUST mul def

    PRESERVE {
	XSCALE YSCALE lt
	    { /YSCALE XSCALE def }
	    { /XSCALE YSCALE def }
	ifelse
    } if

    /XOFF { % correction for excess margin in landscape X direction
	PAGEHEIGHT LPAGEWIDTH 2 mul XSCALE mul sub	% actual inch units
	dup 0 le
	    { pop 0 }
	    { XSCALE div 72 mul 4 div }		% virtual point units, 4 parts
	ifelse
    } bind def

    /YOFF { % correction for excess margin in landscape Y direction
	PAGEWIDTH LPAGEHEIGHT YSCALE mul sub	% actual inch units
	dup 0 le
	    { pop 0 }
	    { YSCALE div 72 mul 2 div }		% virtual point units, 2 parts
	ifelse
    } bind def

    /LEFT   {
	XOFFSET XSCALE div 72 mul	% actual inches --> virtual points
	LPAGEWIDTH PAGEWIDTH sub 72 mul add
	XOFF add
    } bind def

    /BOTTOM {
	PAGEWIDTH YSCALE div 72 mul neg
	YOFFSET YSCALE div 72 mul add	% actual inches --> virtual points
	LPAGEHEIGHT PAGEHEIGHT sub 72 mul add
	YOFF add
    } bind def

    /INITPAGE { 90 rotate XSCALE YSCALE scale LEFT BOTTOM translate } bind def
    /RIGHTORIGIN {
	PAGEHEIGHT 2 div RIGHTFUDGE add
	XSCALE div 72 mul 0 translate
    } bind def

end


/showpage {
    TWOUPDICT begin
	LEFTSIDE {
	    systemdict /initgraphics get exec
	    INITPAGE
	    RIGHTORIGIN
	    /LEFTSIDE false def
	} {
	%%%%    systemdict /showpage get exec	% implicit initgraphics
	    INITPAGE
	    /LEFTSIDE true def
	} ifelse
    end
} def



TWOUPDICT begin
    INITPAGE
    LEFTSIDE not { RIGHTORIGIN } if
end
